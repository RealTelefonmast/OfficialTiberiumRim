	<TR.TRThingDef ParentName="MechFactionGun">
		<defName>Nod_AvatarLaser</defName>
		<label>Avatar Laser</label>
		<graphicData>
			<texPath>Items/Weapons/Gun_Blank</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Interact_Rifle</soundInteract>
		<statBases>
			<MaxHitPoints>250</MaxHitPoints>
			<AccuracyTouch>0.45</AccuracyTouch>
			<AccuracyShort>0.55</AccuracyShort>
			<AccuracyMedium>0.65</AccuracyMedium>
			<AccuracyLong>0.45</AccuracyLong>
			<RangedWeapon_Cooldown>3</RangedWeapon_Cooldown>
		</statBases>
		<verbs>
			<li Class="TeleCore.VerbProperties_Extended">
				<verbClass>TeleCore.Verb_Beam</verbClass>
				<beamProps>
					<damageDef>TRLaser</damageDef>
					<isStatic>false</isStatic>
					<damageBaseOverride>35</damageBaseOverride>
					<stoppingPowerOverride>0.2</stoppingPowerOverride>
					<armorPenetrationOverride>0.5</armorPenetrationOverride>
					<impactEffecter>BeamHit</impactEffecter>
					<impactFilth IsNull="True"/>
				</beamProps>
				<originOffsetPerShot>
					<li>(0.15, 0, 0)</li>
					<li>(0, 0, 0)</li>
					<li>(-0.15, 0, 0)</li>
				</originOffsetPerShot>
				<hasStandardCommand>true</hasStandardCommand>
				<warmupTime>2</warmupTime>
				<range>29.9</range>
				<minRange>3.5</minRange>
				<burstShotCount>3</burstShotCount>
				<soundCast>Nod_AvatarLaser</soundCast>
				<soundCastTail>Shot_Nod_GunTail</soundCastTail>
				<muzzleFlashScale>9</muzzleFlashScale>
				<targetParams>
					<canTargetLocations>true</canTargetLocations>
				</targetParams>
				<ai_IsBuildingDestroyer>true</ai_IsBuildingDestroyer>
				
				<!--<soundCastBeam></soundCastBeam>-->
				<beamFullWidthRange>6.9</beamFullWidthRange>
				<beamWidth>1.25</beamWidth>
				<ticksBetweenBurstShots>8</ticksBetweenBurstShots>	
				<beamGroundFleckDef>Fleck_BeamBurn</beamGroundFleckDef>
				<beamLineFleckDef>Fleck_BeamSpark</beamLineFleckDef>
				<beamFleckChancePerTick>0.32</beamFleckChancePerTick>
				<beamMaxDeviation>0</beamMaxDeviation>
				<beamCurvature>0.25</beamCurvature>
				<beamMoteDef>Mote_NodGunStrongBeam</beamMoteDef>
				<beamEndEffecterDef>AnyBeam_End</beamEndEffecterDef>
				<beamChanceToStartFire>0.85</beamChanceToStartFire>
				<beamChanceToAttachFire>0.85</beamChanceToAttachFire>
				<beamFireSizeRange>0.55~0.85</beamFireSizeRange>
				<beamStartOffset>0.15</beamStartOffset>
				<beamLineFleckChanceCurve>
					<points>
						<li>(0, 0)</li>
						<li>(0.65, 0.4)</li>
						<li>(1, 0.75)</li>
					</points>
				</beamLineFleckChanceCurve>
			</li>
		</verbs>
		<weaponTags>
			<li>NodAvatarSetA</li>
		</weaponTags>
	</TR.TRThingDef>
	
