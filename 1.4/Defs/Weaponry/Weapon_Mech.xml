<Defs>

	<ThingDef Name="MechFactionGun" ParentName="BaseWeapon" Abstract="True">
		<tradeability>None</tradeability>
		<techLevel>Spacer</techLevel>
		<destroyOnDrop>True</destroyOnDrop>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
