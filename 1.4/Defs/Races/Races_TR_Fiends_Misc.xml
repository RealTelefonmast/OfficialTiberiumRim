	<ThingDef ParentName="FiendBase">
		<defName>Crawler</defName>
		<label>crawler</label>
		<description>A huge snake. This one seems to be infected with Tiberium.</description>
		<statBases>
			<MoveSpeed>4.85</MoveSpeed>
			<MarketValue>1200</MarketValue>
			<ComfyTemperatureMin>-5</ComfyTemperatureMin>
			<ComfyTemperatureMax>60</ComfyTemperatureMax>
			<Mass>100</Mass>
			<MeatAmount>50</MeatAmount>
			<PainShockThreshold>1</PainShockThreshold>
		</statBases>
		<tools>
			<li>
				<label>venom-fangs</label>
				<capacities>
					<li>ToxicBite</li>
				</capacities>
				<power>25</power>
				<cooldownTime>4</cooldownTime>
				<linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>100</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>10</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>EggCobraFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>7.5</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>
				<min>1</min>
				<max>2</max>
				</eggCountRange>
			</li>
		</comps>
		<race>
			<body>Snake</body>
			<predator>true</predator>
			<maxPreyBodySize>1.5</maxPreyBodySize>
			<baseBodySize>1</baseBodySize>
			<baseHungerRate>0.25</baseHungerRate>
			<baseHealthScale>2</baseHealthScale>
			<foodType>CarnivoreAnimalStrict</foodType>
			<wildness>0.9</wildness>
			<manhunterOnTameFailChance>1</manhunterOnTameFailChance>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<nameOnTameChance>1</nameOnTameChance>
			<lifeExpectancy>20</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.15</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.5</minAge>
					<soundWounded>Pawn_Cobra_Wounded</soundWounded>
					<soundDeath>Pawn_Cobra_Death</soundDeath>
					<soundCall>Pawn_Cobra_Call</soundCall>
					<soundAngry>Pawn_Cobra_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
	</ThingDef>

