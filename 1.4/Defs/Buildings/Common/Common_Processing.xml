				<mechRecipes>
					<li>MakeHarvesterCommon</li>
				</mechRecipes>
			</li>
			<li Class="TR.CompProperties_TNSRefinery">
				<networks>
					<li>
						<networkDef>TiberiumNetwork</networkDef>
						<volumeConfig>
							<capacity>1500</capacity>
							<leaveContainer>true</leaveContainer>
							<storeEvenly>false</storeEvenly>
							<values>
								<allowedValues>
									<li>TibGreen</li>
									<li>TibBlue</li>
									<li>TibRed</li>
									<li>TibSludge</li>
								</allowedValues>
							</values>
