	<!--Refinery-->
	<TR.TRThingDef ParentName="RefineryBase_TR">
		<defName>TR_TiberiumRefinery</defName>
		<label>tiberium refinery</label>
		<description>This refinery will create a Harvester upon construction, which will harvest nearby Tiberium, return and store it inside of the refinery.</description>
		<size>(3,4)</size>
		<graphicData>
			<texPath>Buildings/Common/Network/Refinery/Refinery</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(4.15,4.15)</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>3000</WorkToBuild>
			<Flammability>0.85</Flammability>
		</statBases>
		<costList>
			<Steel>500</Steel>
			<ComponentIndustrial>10</ComponentIndustrial>
		</costList>
		<requisites>
			<tiberiumResearchTasks>
				<li>TiberiumRefining_T4</li>
			</tiberiumResearchTasks>
		</requisites>
