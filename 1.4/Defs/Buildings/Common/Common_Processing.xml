								<_BlendOpacity>0.5</_BlendOpacity>
								<MySrcMode>1</MySrcMode>
								<MyDstMode>1</MyDstMode>
							</shaderParameters>
							<drawSize>(4.15,4.15)</drawSize>
						</graphicData>
						<textureParams>
							<_BaseTex>Buildings/Common/Network/Refinery/VFX/Base</_BaseTex>
							<_BlendTex1>Buildings/Common/Network/Refinery/VFX/FlowBlend1</_BlendTex1>
							<_BlendTex2>Buildings/Common/Network/Refinery/VFX/FlowBlend2</_BlendTex2>
							<_FlowMap>Buildings/Common/Network/Refinery/VFX/FlowMap</_FlowMap>
							<_MaskTex>Buildings/Common/Network/Refinery/VFX/FlowMask</_MaskTex>
						</textureParams>
						<fxMode>Dynamic</fxMode>
					</li>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Network/Refinery/RefineryOL</texPath>
							<graphicClass>Graphic_Multi</graphicClass>
							<drawSize>(4.15,4.15)</drawSize>
						</graphicData>
					</li>
				</fxLayers>
			</li>
		</comps>
		<inspectorTabs>
			<li>TR.ITab_MechConstruction</li>
		</inspectorTabs>
		<placeWorkers>
			<li>TR.PlaceWorker_Gap</li>
		</placeWorkers>
		<hasInteractionCell>true</hasInteractionCell>
		<interactionCellOffset>(0,0,-2)</interactionCellOffset>
		<interactionCellIcon>RefineryCell</interactionCellIcon>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>Common</groupDef>
				<category>Processing</category>
			</li>
		</modExtensions>
