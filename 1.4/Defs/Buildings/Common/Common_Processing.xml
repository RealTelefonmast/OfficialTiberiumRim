<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--Network Controller-->
	<TR.TRThingDef ParentName="NetworkStationBase">
		<defName>TR_TiberiumNetworkController</defName>
		<label>Tiberium Network Controller</label>
		<description>This structure is the main part of any Tiberium network, it acts as a pump for the entire networks and ensures flow while powered.</description>
		<thingCategories Inherit="false" />
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<graphicData>
			<texPath>Buildings/Common/Network/TNC/TNC</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(4.75,4.75)</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>5000</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-10</Beauty>
		</statBases>
		<size>(4,4)</size>
		<costList>
			<Steel>250</Steel>
			<ComponentIndustrial>15</ComponentIndustrial>
		</costList>
		<requisites>
			<tiberiumResearchTasks>
				<li>TiberiumRefining_T5</li>
			</tiberiumResearchTasks>
		</requisites>
		<comps>
			<li Class="TR.CompProperties_TNS">
				<networks>
					<li>
						<networkDef>TiberiumNetwork</networkDef>
						<volumeConfig>
							<capacity>2500</capacity>
							<!--<dropContents>true</dropContents>-->
							<leaveContainer>true</leaveContainer>
							<storeEvenly>true</storeEvenly>	
							<values>
								<fromCollection>TiberiumNetwork</fromCollection>
							</values>
						</volumeConfig>
						<roles>
							<li>Transmitter</li>
							<li>Controller</li>
						</roles>
					</li>
					<li>
						<networkDef>WasteNetwork</networkDef>
						<volumeConfig>
							<capacity>2500</capacity>
							<!--<dropContents>true</dropContents>-->
							<leaveContainer>true</leaveContainer>
							<storeEvenly>true</storeEvenly>	
							<values>
								<fromCollection>WasteNetwork</fromCollection>
							</values>
						</volumeConfig>
						<roles>
							<li>Transmitter</li>
							<li>Controller</li>
						</roles>
					</li>
				</networks>
				<generalIOConfig>
					<pattern>
					##XX##|
					##++##|
					X++++X|
					X++++X|
					##++##|
					##XX##
					</pattern>
				</generalIOConfig>
			</li>
			<li Class="TeleCore.CompProperties_FX"> 
				<fxLayers>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Network/TNC/TNC_Displays</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<shaderType>MoteGlow</shaderType>
							<drawSize>(4.75,4.75)</drawSize>
						</graphicData>
						<needsPower>true</needsPower>
					</li>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Network/TNC/TNC_Overlay</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<shaderType>MoteGlow</shaderType>
							<drawSize>(4.75,4.75)</drawSize>
						</graphicData>
						<fxMode>Dynamic</fxMode>
						<needsPower>false</needsPower>
					</li>
				</fxLayers>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>2</glowRadius>
				<glowColor>(120,255,150,0)</glowColor>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>255</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<tickerType>Normal</tickerType>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>	
		<fillPercent>1</fillPercent>
		<rotatable>false</rotatable>			
		<surfaceType>Item</surfaceType>
		<placeWorkers>
			<li>TR.PlaceWorker_Once</li>
		</placeWorkers>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>Common</groupDef>
				<category>Processing</category>
			</li>
		</modExtensions>
	</TR.TRThingDef>
	
	<!--Silo-->
	<TR.TRThingDef ParentName="NetworkStationBase">
		<defName>TR_TiberiumSilo</defName>
		<label>tiberium silo</label>
		<description>Stores Tiberium and distributes it to other Tiberium structures when necessary.</description>
		<size>(3,3)</size>
		<costList>
			<Steel>100</Steel>
		</costList>
		<graphicData>
			<texPath>Buildings/Common/Network/Silo/Silo</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
			<shadowData>
				<volume>(1.9, 1, 1.68)</volume>
				<offset>(-0.03,0,-0.1)</offset>
			</shadowData>
		</graphicData>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<blockWind>true</blockWind>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>
		<fillPercent>1</fillPercent>
		<tickerType>Normal</tickerType>
		<statBases>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToBuild>2500</WorkToBuild>
			<Mass>20</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>-25</Beauty>
		</statBases>
		<requisites>
			<tiberiumResearchTasks>
				<li>TiberiumRefining_T2</li>
			</tiberiumResearchTasks>
		</requisites>
		<comps>
			<li Class="TR.CompProperties_TNS">
				<networks>
					<li>
						<networkDef>TiberiumNetwork</networkDef>
						<volumeConfig>
							<capacity>2500</capacity>
							<!--<dropContents>true</dropContents>-->
							<leaveContainer>true</leaveContainer>
							<storeEvenly>true</storeEvenly>	
							<values>
								<allowedValues>
									<li>TibGreen</li>
									<li>TibBlue</li>
									<li>TibRed</li>			
									<li>TibSludge</li>	
								</allowedValues>
							</values>
						</volumeConfig>
						<roles>
							<li>Transmitter</li>
							<li>Storage</li>
						</roles>
					</li>
				</networks>
				<generalIOConfig>
					<pattern>
					##X##|
					##+##|
					X+++X|
					##+##|
					##X##
					</pattern>
				</generalIOConfig>
			</li>
			<li Class="TeleCore.CompProperties_FX"> 
				<fxLayers>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Network/Silo/Silo_Filled</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<shaderType>MaskedFlowMap</shaderType>
							<drawSize>(3,3)</drawSize>
							<shaderParameters>
								<_FlowSpeed>0.375</_FlowSpeed>
								<_BlendOpacity>0.5</_BlendOpacity>
								<MySrcMode>1</MySrcMode>
								<MyDstMode>1</MyDstMode>
							</shaderParameters>
						</graphicData>
						<textureParams>
							<_BaseTex>Buildings/Common/Network/Silo/VFX/Base</_BaseTex>
							<_BlendTex1>Buildings/Common/Network/Silo/VFX/FlowBlend1</_BlendTex1>
							<_BlendTex2>Buildings/Common/Network/Silo/VFX/FlowBlend2</_BlendTex2>
							<_FlowMap>Buildings/Common/Network/Silo/VFX/FlowMap</_FlowMap>
							<_MaskTex>Buildings/Common/Network/Silo/VFX/FlowMask</_MaskTex>
						</textureParams>
						<fxMode>Dynamic</fxMode>
						<categoryTag>FXNetwork</categoryTag>
						<layerTag>Container</layerTag>
					</li>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Network/Silo/SiloOL</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(3,3)</drawSize>
						</graphicData>
						<categoryTag>FXNetwork</categoryTag>
						<layerTag>Container</layerTag>
					</li>
				</fxLayers>
			</li>
		</comps>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<constructEffect>ConstructMetal</constructEffect>
		<canOverlapZones>false</canOverlapZones>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>Common</groupDef>
				<category>Processing</category>
			</li>
		</modExtensions>
	</TR.TRThingDef>
	
