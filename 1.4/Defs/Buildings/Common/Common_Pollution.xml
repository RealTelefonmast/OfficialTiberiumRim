		<fillPercent>1</fillPercent>
		<rotatable>true</rotatable>			
		<surfaceType>Item</surfaceType>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>Common</groupDef>
				<category>Pollution</category>
			</li>
		</modExtensions>
	</TiberiumRim.TRThingDef>
	
