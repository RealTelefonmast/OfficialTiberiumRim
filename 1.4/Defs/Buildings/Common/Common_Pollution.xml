<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<TiberiumRim.TRThingDef ParentName="StationBase_TR">
		<defName>AtmosphericPumpTwo</defName>
		<label>Atmospheric Pump 2</label>
		<description>.</description>
		<thingCategories Inherit="false" />
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Buildings/Common/Network/Node/Node</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2.25,4.5)</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToBuild>1000</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-5</Beauty>
		</statBases>
		<size>(2,4)</size>
		<costList>
			<Steel>100</Steel>
			<ComponentIndustrial>5</ComponentIndustrial>
		</costList>
		<requisites>
			<tiberiumResearchTasks>
				<li>TiberiumRefining_T2</li>
			</tiberiumResearchTasks>
		</requisites>
		<comps>
			<li Class="TeleCore.CompProperties_NetworkStructure">
				<!--<compClass>TiberiumRim.Comp_ANS_Filter</compClass>-->
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<networkRoles>
							<li>Transmitter</li>
							<li>Controller</li>
							<li>Storage</li>
						</networkRoles>
						<containerConfig>
							<baseCapacity>1000</baseCapacity>
							<dropContents>false</dropContents>
						</containerConfig>
						<defFilter>
							<values>
								<li>Network_TibPollution</li>
							</values>
						</defFilter>
					</li>
				</networks>
				<!--<generalIOPattern>++|++</generalIOPattern>-->
			</li>
			<li Class="TeleCore.CompProperties_FX"> 
				<fxLayers>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Network/Node/Overlay1</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<shaderType>MoteGlow</shaderType>
							<drawSize>(2.25,4.5)</drawSize>
						</graphicData>
						<fxMode>Dynamic</fxMode>
						<needsPower>false</needsPower>
					</li>
					<li><skip>true</skip></li>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Network/Node/Overlay2</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<shaderType>MoteGlow</shaderType>
							<drawSize>(2.25,4.5)</drawSize>
						</graphicData>
						<needsPower>true</needsPower>
					</li>
				</fxLayers>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>1000</basePowerConsumption>
			</li>
		</comps>
		<rotatable>true</rotatable>
		<tickerType>Never</tickerType>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>	
		<fillPercent>1</fillPercent>			
		<surfaceType>Item</surfaceType>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>Common</groupDef>
				<category>Misc</category>
			</li>
		</modExtensions>
	</TiberiumRim.TRThingDef>
	
	<TiberiumRim.TRThingDef ParentName="StationBase_TR">
		<defName>PollutionFilter</defName>
		<label>pollution filter</label>
		<description>.</description>
		<thingCategories Inherit="false" />
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<drawerType>MapMeshAndRealTime</drawerType>
		<tickerType>Normal</tickerType>
		<graphicData>
			<texPath>Buildings/Common/Pollution/Filter</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2.25,2.25)</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>5000</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-10</Beauty>
		</statBases>
		<size>(2,2)</size>
		<costList>
			<Steel>200</Steel>
			<ComponentIndustrial>10</ComponentIndustrial>
		</costList>
		<requisites>
			<tiberiumResearchTasks>
				<li>TiberiumRefining_T5</li>
			</tiberiumResearchTasks>
		</requisites>
		<comps>
			<li Class="TeleCore.CompProperties_NetworkStructure">
				<compClass>TiberiumRim.Comp_ANS_TiberiumFilter</compClass>
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<networkRoles>
							<li>Transmitter</li>
							<li>Requester</li>
							<li>Storage</li>
						</networkRoles>
						<containerConfig>
							<baseCapacity>750</baseCapacity>
							<dropContents>false</dropContents>
						</containerConfig>
						<defFilter>
							<values>
								<li>Network_TibPollution</li>
							</values>
						</defFilter>
					</li>
					<li>
						<networkDef>TiberiumNetwork</networkDef>
						<networkRoles>
							<li>Transmitter</li>
							<li>Producer</li>
						</networkRoles>
						<containerConfig>
							<baseCapacity>1250</baseCapacity>
							<dropContents>false</dropContents>
							<leaveContainer>true</leaveContainer>
							<storeEvenly>false</storeEvenly>	
						</containerConfig>
						<defFilter>
							<values>
								<li>TibSludge</li>
							</values>
						</defFilter>
					</li>
				</networks>
				<generalIOPattern>++|++</generalIOPattern>
			</li>
			<li Class="TeleCore.CompProperties_FX"> 
				<fxLayers>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Pollution/Filter_Overlay</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<shaderType>MoteGlow</shaderType>
							<drawSize>(2.25,2.25)</drawSize>
						</graphicData>
						<fxMode>Dynamic</fxMode>
						<needsPower>true</needsPower>
					</li>
				</fxLayers>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>2</glowRadius>
				<glowColor>(255,155,0)</glowColor>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>255</basePowerConsumption>
			</li>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>	
		<fillPercent>1</fillPercent>
		<rotatable>false</rotatable>			
		<surfaceType>Item</surfaceType>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>Common</groupDef>
				<category>Pollution</category>
			</li>
		</modExtensions>
	</TiberiumRim.TRThingDef>
	
	<TiberiumRim.TRThingDef ParentName="StationBase_TR">
		<defName>AtmosphericVentInTwo</defName>
		<label>pollution vent (intake)</label>
		<description>.</description>
		<thingCategories Inherit="false" />
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<drawerType>MapMeshAndRealTime</drawerType>
		<tickerType>Normal</tickerType>
		<uiIconPath>Buildings/Common/Pollution/Intake/icon</uiIconPath>
		<graphicData>
			<texPath>Buildings/Common/Pollution/Intake/base</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1.25,1.25)</drawSize>
			<drawOffset>(0,0,0.1)</drawOffset>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>1500</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-10</Beauty>
		</statBases>
		<size>(1,1)</size>
		<costList>
			<Steel>50</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="TAE.CompProperties_ANS_AirVent">
				<filter>
					<acceptedAtmospheres>	
						<li>Atmosphere_TibPollution</li>					
					</acceptedAtmospheres>
				</filter>
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<networkRoles>
							<li>Transmitter</li>
							<li>Producer</li>
						</networkRoles>
						<containerConfig>
							<baseCapacity>100</baseCapacity>
							<dropContents>false</dropContents>
						</containerConfig>
						<defFilter>
							<li>Network_TibPollution</li>	
						</defFilter>
					</li>
				</networks>
				<generalIOPattern>+</generalIOPattern>
			</li>
			<li Class="TeleCore.CompProperties_FX"> 
				<fxLayers>
					<li><skip>true</skip></li>
					<li><skip>true</skip></li>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Pollution/Intake/fan</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(1.25,1.25)</drawSize>
							<drawOffset>(0,0,0.1)</drawOffset>
						</graphicData>
						<fxMode>Dynamic</fxMode>
						<rotate>
							<rotationSpeed>75</rotationSpeed>
						</rotate>
						<drawOffset>(0,0,0.1)</drawOffset>
						<needsPower>false</needsPower>
					</li>
					<li>
						<graphicData>
							<texPath>Buildings/Common/Pollution/Intake/top</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(1.25,1.25)</drawSize>
							<drawOffset>(0,0,0.1)</drawOffset>
						</graphicData>
						<needsPower>false</needsPower>
					</li>
				</fxLayers>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>45</basePowerConsumption>
			</li>
		</comps>
		<requisites>
			<tiberiumResearchTasks>
				<li>TiberiumRefining_T5</li>
			</tiberiumResearchTasks>
		</requisites>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>	
		<fillPercent>1</fillPercent>
		<rotatable>true</rotatable>			
		<surfaceType>Item</surfaceType>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>Common</groupDef>
				<category>Pollution</category>
			</li>
		</modExtensions>
	</TiberiumRim.TRThingDef>
	
	<TiberiumRim.TRThingDef ParentName="StationBase_TR">
		<defName>AtmosphericVentOutTwo</defName>
