<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<TR.AnimalMutationDef>
		<defName>TiberiumFiendMutations</defName>
		<conversions>
			<li>
				<toPawn>Thrimbo</toPawn>
				<fromPawn>
					<li>Thrumbo</li>
				</fromPawn>
			</li>
			<li>
				<toPawn>BigTiberiumFiend</toPawn>
				<fromPawn>
					<li>Bear_Grizzly</li>
					<li>Bear_Polar</li>
				</fromPawn>
			</li>
			<li>
				<toPawn>TiberiumFiend</toPawn>
				<fromPawn>
					<li>Wolf_Timber</li>
					<li>Wolf_Arctic</li>
				</fromPawn>
			</li>
			<li>
				<toPawn>SmallTiberiumFiend</toPawn>
				<fromPawn>
					<li>Fox_Fennec</li>
					<li>Fox_Red</li>
					<li>Fox_Arctic</li>
				</fromPawn>
			</li>
			<li>
				<toPawn>Tiffalo</toPawn>
				<fromPawn>
					<li>Muffalo</li>
				</fromPawn>
			</li>
			<li>
				<toPawn>Spiner</toPawn>
				<fromPawn>
					<li>Warg</li>
				</fromPawn>
			</li>
			<li>
				<toPawn>BoomFiend</toPawn>
				<fromPawn>
					<li>Boomrat</li>
				</fromPawn>
			</li>
			<li>
				<toPawn>Crawler</toPawn>
				<fromPawn>
					<li>Cobra</li>
				</fromPawn>
			</li>
			<li>
				<toPawn>TibScarab</toPawn>
				<fromPawn>
					<li>Megascarab</li>
				</fromPawn>
			</li>
		</conversions>
	</TR.AnimalMutationDef>

