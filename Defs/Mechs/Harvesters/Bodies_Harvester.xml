		<corePart>
			<def>HarvesterContainer</def>
			<height>Middle</height>
			<depth>Outside</depth>
			<parts>
				<li>
					<def>HarvesterCockpit</def>
					<coverage>0.2</coverage>
					<height>Top</height>
					<groups>
						<li>HeadAttackTool</li>
					</groups>
					<parts>
						<li>
							<def>Harvester_Collector</def>
							<coverage>0.10</coverage>
							<height>Top</height>
							<parts>
								<li>
									<def>Harvester_CollectorArm</def>
									<customLabel>left collector arm</customLabel>
									<coverage>0.045</coverage>
									<groups>
										<li>LeftHand</li>
									</groups>
								</li>
								<li>
									<def>Harvester_CollectorArm</def>
									<customLabel>right collector arm</customLabel>
