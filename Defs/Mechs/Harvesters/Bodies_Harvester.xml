						</li>
					</parts>
				</li>
				<li>
					<def>Harvester_Track</def>
					<customLabel>right track</customLabel>
					<coverage>0.30</coverage>
					<parts>
						<li>
							<def>Harvester_Wheel</def>
							<customLabel>right first wheel</customLabel>
							<coverage>0.075</coverage>
						</li>
						<li>
							<def>Harvester_Wheel</def>
