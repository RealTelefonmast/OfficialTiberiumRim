			<MoveSpeed>4.6</MoveSpeed>
			<MarketValue>100</MarketValue>
			<ComfyTemperatureMin>-15</ComfyTemperatureMin>
		</statBases>
		<tools>
			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
