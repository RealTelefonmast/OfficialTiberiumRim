		<race>
			<body>QuadrupedAnimalWithPawsAndTail</body>
			<maxPreyBodySize>2.3</maxPreyBodySize>
			<petness>0.1</petness>
			<baseBodySize>0.85</baseBodySize>
			<baseHungerRate>0.18</baseHungerRate>
			<baseHealthScale>1.2</baseHealthScale>
			<foodType>CarnivoreAnimal</foodType>
			<nameOnTameChance>1</nameOnTameChance>
			<trainability>Advanced</trainability>
			<wildness>0.85</wildness>
			<nuzzleMtbHours>640</nuzzleMtbHours>
			<manhunterOnTameFailChance>0.10</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.10</manhunterOnDamageChance>
			<gestationPeriodDays>26</gestationPeriodDays>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(2, 1.7)</li>
					<li>(2.5, 0.7)</li>
					<li>(3, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeExpectancy>10</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.2</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.45</minAge>
					<soundWounded>Pawn_Dog_Injured</soundWounded>
					<soundDeath>Pawn_Dog_Death</soundDeath>
					<soundCall>Pawn_Dog_Call</soundCall>
					<soundAngry>Pawn_Dog_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
